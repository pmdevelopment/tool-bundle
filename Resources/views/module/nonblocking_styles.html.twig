{% if assets is defined and assets is iterable %}
    {# Do not forget to include noscript tag into head! #}
    <script type=text/javascript>
        !function () {
            var i, n, m, me, el = [];
            var s = {{ assets|json_encode()|raw }};

            var t = document.getElementsByTagName('head')[0];
            for (i = 0; i < s.length; i++) {
                n = document.createElement("link");

                n.rel = "stylesheet", n.href = s[i];
                n.media = "only x";
                t.appendChild(n);

                el.push(n);
            }

            window.setTimeout(function () {
                for (i = 0; i < el.length; i++) {
                    el[i].media = "all";
                }
            }, 10);
        }();
    </script>
{% endif %}